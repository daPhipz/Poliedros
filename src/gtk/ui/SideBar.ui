<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="SideBar" parent="GtkBox">
    <property name="orientation">1</property>
    <property name="vexpand">true</property>
    <property name="hexpand">true</property>
    <child>
      <object class="GtkListBox" id="_sidebar_list">
        <property name="show-separators">true</property>
        <property name="vexpand">true</property>
        <property name="hexpand">true</property>
        <style>
          <class name="log_background"/>
        </style>       
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <style>
          <class name="toolbar"/>
        </style>
        <child>
          <object class="GtkButton" id="_clear_history_button">
            <property name="icon-name">edit-clear-all-symbolic</property>
            <property name="tooltip-text" translatable="true">Clear results</property>
            <property name="valign">2</property>
            <property name="halign">1</property>
            <property name="hexpand">false</property>
            <style>
              <class name="destructive-action"/>
            </style>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>