<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0" />
  <requires lib="Adw" version="1.0" />
  <template class="RemapInput" parent="AdwExpanderRow">
    <property name="title">Input :Dell Computer Corp. KB212-B Quiet Key Keyboard</property>
    <property name="subtitle">Crtl+C</property>    
    <child type="suffix">
      <object class="GtkBox">
        <child>
          <object class="GtkButton">
            <property name="icon-name">list-add-symbolic</property>
            <property name="valign">3</property>
            <style>
              <class name="flat" />
            </style>
          </object>
        </child>
        <child>
          <object class="GtkButton">
            <property name="icon-name">user-trash-symbolic</property>
            <property name="valign">3</property>
            <style>
              <class name="flat" />
            </style>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="RemapOutput"></object>
    </child>
    <style>
      <class name="property" />
    </style>    
  </template>
</interface>